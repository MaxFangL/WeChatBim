<swiper indicator-dots="{{swiperCon.indicatorDots}}"
  autoplay="{{swiperCon.autoplay}}" interval="{{swiperCon.interval}}" duration="{{swiperCon.duration}}" circular="{{swiperCon.circular}}" previous-margin="{{swiperCon.previousMargin}}" next-margin="{{swiperCon.nextMargin}}">
  <block wx:for="{{swiperCon.imgUrls}}" wx:key="*this">
    <swiper-item>
      <image src="{{item}}" class="slide-image" width="355" height="150"/>
    </swiper-item>
  </block>
</swiper>
